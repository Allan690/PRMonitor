{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["warn", { "code": 100, "ignoreUrls": true }],
        "consistent-return": 0,
       "comma-dangle": 0,
        "no-fallthrough": 0,
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "no-trailing-spaces": ["error", { "skipBlankLines": true }],
        "no-underscore-dangle": "off",
        "eol-last": ["error", "always"]
    }
}
